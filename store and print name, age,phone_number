//store name,age and phone number and show them in terminal

#include<stdio.h>
int main()
{
	char name[20];int phone,age;
	printf("enter name> ");
	gets(name); 
	printf("enter age> ");
	scanf("%d",&age);
	printf("enter phone number> ");
	scanf("%d",&phone);
	
	printf("name : %s \n",name);
	printf("age : %d \n",age);
	printf("phone number : %d \n",phone);
	
	
}
