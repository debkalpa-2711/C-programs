#include <stdio.h>
int main()
{
    int age , phone ;
    char name[50];
    printf("enter name >> ");
    scanf("%s",&name);
    printf("enter age>>");
    scanf("%d",&age);
    printf("enter phone number>> ");
    scanf("%d",&phone);
    
    printf("%s\n",name);
    printf("%d\n",age);
    printf("%d\n",phone);
    
    return 0;
}
